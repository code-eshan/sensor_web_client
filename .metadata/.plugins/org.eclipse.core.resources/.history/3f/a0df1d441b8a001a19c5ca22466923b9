    <table>
    
    <tbody id="data">
    
    </tbody>
    
    </table>
    
    <script>
    
    fetch("http://localhost:8080/sensorRest/webapi/sensors").then(
    		res=>{
    			res.json().then(
    				data => {
    					console.log(data);
    					if(data.length > 0) {
    						var temp = "";
    						
    						//----- star for loop
    						
    						data.forEach((u) => {
    							temp += "<tr>";
    							temp += "<td>" + u;
    						})
    						//----- close for loop
    					}
    				}		
    			)
    		}
    )
    
    </script>
    
    <p>Visit <a href="http://jersey.java.net">Project Jersey website</a>
    for more information on Jersey!
